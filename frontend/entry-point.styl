textarea
	resize: none;

body
	height: 100%;
	margin: 0;
	min-width: 1024px;
	min-height: 768px;

html
	height: 100%;

$icon = {
	plus: url('data:image/svg+xml;base64,'),
	arrow: url('data:image/svg+xml;utf8;base64,'),
	red-arrow: url('data:image/svg+xml;utf8;base64,'),
	cross: url('data:image/svg+xml;base64,'),
	logout: url('data:image/svg+xml;base64,'),
	loader: url('data:image/svg+xml;base64,'),
	generate: url('data:image/svg+xml;base64,'),
	sort-dark: url('data:image/png;base64,'),
  sort-white: url('data:image/png;base64,'),
  red-cross: url('data:image/svg+xml;base64,'),
  green-check: url('data:image/svg+xml;base64,')
}

$weight = {
	thin: 100,
	regular: 400,
	medium: 500,
	bold: 700
}

$color = {
	main: #0f214c,
	error: #ff0000,
	success: #008000,
	white: #ffffff,
	disabled: #eee,
	match: yellow
}

@import "components/**/*.styl";
